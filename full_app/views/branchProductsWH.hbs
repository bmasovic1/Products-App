    <h1 >Product to Branch</h1>
    {{#if branchId}}
    <div>
        <h2 style="color: blue">{{branchId}}</h4 >
    </div>
    <div class="wrapper">
    {{#if alert}}
    <div>
        <br>
        <h4 style="color: green">{{alert}}</h4 >
    </div>
    {{/if}}
    {{#each productsO}}
            <form action="/warehouse/branches/addProduct/{{this.branchId}}/{{this.id}}" method="post">
                <div class="input-box">
                    <input type="text" placeholder="Name:" required name="name" value="{{this.name}}" required readonly/>
                </div>
                <div class="input-box">
                 <div class="container">
                    <span class="minus">-</span>
                    <span class="num">
                        1
                    </span>
                    <input class="inp" type="text" name="quantity" value="1" style="display: none;"/>
                    <span class="plus">+</span>
                  </div>
                </div>
                <div class="input-box">
                    <input type="text" placeholder="Quantity:" name="quantity2" value="{{this.quantity}}" required/>
                </div>
                <div class="input-box">
                    <input type="text" placeholder="Unit:" name="unit" value="{{this.unit}}"/>
                </div>
                <div class="input-box button">
                    <input type="Submit" value="Add to branch">
                </div>
            </form> 
    {{/each}}
     {{/if}}
     {{#if noProducts}}
     <div>
         <h4>{{noProducts}}</h4>
     </div>
     {{/if}}
  <div class="d-flex justify-content-between">
         <div class="text-muted">
            
         </div>
         <div class="d-flex flex-column justify-content-end align-items-end">
             <div class="d-flex px-3 pr-md-5 py-1 subtotal">
                 <button class="btn" class="px-4"><a href="/warehouse/branches"
                  style="color: black; text-decoration: none"><b> <-BRANCH </b></a></button>
             </div>
         </div>
     </div> 
 </div>

  <script>

            const plus = document.querySelector(".plus"),
             minus = document.querySelector(".minus"),
             num = document.querySelector(".num");
             inp = document.querySelector(".inp");
             let a = 1;
             plus.addEventListener("click", ()=>{
               a++;
               a = (a < 10) ? 0 + a : a;
               num.innerText = a;
               inp.setAttribute('value',a);
                 console.log("da " +inp.textContent)
             });
         
             minus.addEventListener("click", ()=>{
               if(a > 1){
                 a--;
                 a = (a < 10) ? 0 + a : a;
                 num.innerText = a;
                 inp.setAttribute('value',a);
                 console.log("da " +inp.textContent)
               }
             });
           </script>