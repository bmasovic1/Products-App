  <h1>Branches</h1>
<div class="table-container">

  <table class="table">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Operations</th>
      </tr>
    </thead>
    <tbody>
      {{#each branches}}
        <tr>
          <td data-label="ID">{{this.id}}</td>
          <td data-label="name">{{this.name}}</td>
          <td data-label="Operations">
            <a
              href="/warehouse/branches/viewproducts/{{this.id}}"
              type="button"
              style="color: black"
            >View</a>
            <a
              href="/warehouse/branches/brancheproducts/{{this.id}}"
              type="button"
              style="color: black"
            >Add product</a>
           
          </td>

        </tr>
      {{/each}}
    </tbody>
  </table>
</div>