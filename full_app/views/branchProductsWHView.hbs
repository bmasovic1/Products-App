    <h1 >Products of Branch</h1>
    {{#if branch}}
    <div>
        <h2 style="color: blue">{{branch.name}}</h4 >
    </div>
    <div class="wrapper">
    {{#if alert}}
    <div>
        <br>
        <h4 style="color: green">{{alert}}</h4 >
    </div>
    {{/if}}
    {{#each productsO}}
            <form action="/warehouse/branches/addProduct/{{this.branchId}}/{{this.id}}" method="post">
                <div class="input-box">
                    <input type="text" placeholder="Name:" required name="name" value="{{this.name}}" required readonly/>
                </div>
                <div class="input-box">
                    <input type="text" placeholder="Quantity:" name="quantity" value="{{this.quantity}}" required readonly/>
                </div>
                <div class="input-box">
                    <input type="text" placeholder="Unit:" name="unit" value="{{this.unit}}" required readonly/>
                </div>
              <!--  <div class="input-box button">
                    <input type="Submit" value="Add to branch">
                </div>
                !-->
            </form> 
    {{/each}}
     {{/if}}
