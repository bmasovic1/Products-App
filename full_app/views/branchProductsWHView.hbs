    <h1 >Products of Branch</h1>
    {{#if branch}}
    <div>
        <h2 style="color: blue">{{branch.name}}</h4 >
    </div>
    <div class="wrapper">
    {{#if alert}}
    <div>
        <br>
        <h4 style="color: green">{{alert}}</h4 >
    </div>
    {{/if}}
        <div class="table-container">
        <table class="table">
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Quantity</th>
                    <th>Unit</th>
                </tr>
            </thead>
               
            <tbody> {{#each productsO}}
                <tr>
                    <td data-label="Name">{{this.name}}</td> 
                    <td data-label="Quantity">{{this.quantity}}</td>
                    <td data-label="Unit">{{this.unit}}</td>
                </tr>
            </tbody>
             {{/each}}
        </table>
    </div>


    {{/if}}
     <div class="d-flex justify-content-between">
         <div class="text-muted">
            
         </div>
         <div class="d-flex flex-column justify-content-end align-items-end">
             <div class="d-flex px-3 pr-md-5 py-1 subtotal">
                 <button class="btn" class="px-4"><a href="/warehouse/branches"
                  style="color: black; text-decoration: none"><b> <-BRANCH </b></a></button>
             </div>
         </div>
     </div>
